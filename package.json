{
  "name": "about-me-scully",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "scully": "scully",
    "build:hljs": "tsc -p plugins/hljs",
    "scully-build": "rm -rf ./dist && npm run build:hljs && ng build --prod && npm run scully",
    "scully:serve": "scully serve",
    "scully:prod": "rm -rf ./dist && npm run build:hljs && ng build && npm run scully -- --scanRoutes && npm run pwa:regen",
    "pwa:regen": "rm dist/static/ngsw.json;node_modules/.bin/ngsw-config dist/static ./ngsw-config.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.0",
    "@angular/common": "~12.0.0",
    "@angular/compiler": "~12.0.0",
    "@angular/core": "~12.0.0",
    "@angular/forms": "~12.0.0",
    "@angular/platform-browser": "~12.0.0",
    "@angular/platform-browser-dynamic": "~12.0.0",
    "@angular/router": "~12.0.0",
    "@angular/service-worker": "~12.0.0",
    "@gammastream/scully-plugin-http404": "^1.0.1",
    "@nartc/tailwind-schematics": "^1.0.2",
    "@ngneat/transloco": "^2.20.1",
    "@scullyio/init": "0.0.28",
    "@scullyio/ng-lib": "latest",
    "@scullyio/scully": "latest",
    "ng-particles": "^2.1.11",
    "prismjs": "^1.20.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "tsparticles": "^1.18.11",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "10.0.0",
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular/cli": "~12.0.0",
    "@angular/compiler-cli": "~12.0.0",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@types/node": "^12.11.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-scss": "2.1.1",
    "prettier": "2.3.0",
    "scully-plugin-minify-html": "^3.1.2",
    "tailwindcss": "2.1.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4"
  },
  "resolutions": {
    "webpack": "^5.0.0"
  },
  "prettier": {
    "semi": true,
    "tabWidth": 4,
    "singleQuote": true,
    "useTabs": true
  }
}
