{
  "name": "about-me-scully",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "NG_PERSISTENT_BUILD_CACHE=1 ng serve",
    "build": "ng build",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "scully": "scully",
    "build:hljs": "tsc -p plugins/hljs",
    "scully-build": "rm -rf ./dist && npm run build:hljs && ng build --configuration production && npm run scully",
    "scully:serve": "scully serve",
    "scully:prod": "NG_PERSISTENT_BUILD_CACHE=1 rm -rf ./dist && npm run build:hljs && ng build --configuration production && npm run scully -- --scanRoutes && npm run pwa:regen",
    "pwa:regen": "rm dist/static/ngsw.json;node_modules/.bin/ngsw-config dist/static ./ngsw-config.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.4",
    "@angular/common": "~12.0.4",
    "@angular/compiler": "~12.0.4",
    "@angular/core": "~12.0.4",
    "@angular/forms": "~12.0.4",
    "@angular/platform-browser": "~12.0.4",
    "@angular/platform-browser-dynamic": "~12.0.4",
    "@angular/router": "~12.0.4",
    "@angular/service-worker": "~12.0.4",
    "@gammastream/scully-plugin-http404": "^1.0.1",
    "@nartc/tailwind-schematics": "^1.0.2",
    "@ngneat/transloco": "^2.21.0",
    "@scullyio/init": "0.0.28",
    "@scullyio/ng-lib": "latest",
    "@scullyio/scully": "latest",
    "ng-particles": "^2.11.0",
    "patch-package": "^6.4.7",
    "prismjs": "^1.27.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "tsparticles": "^1.28.0",
    "typed.js": "^2.0.12",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "10.0.0",
    "@angular-devkit/build-angular": "~12.0.4",
    "@angular/cli": "~12.0.4",
    "@angular/compiler-cli": "~12.0.4",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@types/node": "^12.11.1",
    "autoprefixer": "^9.8.6",
    "postcss": "^7.0.36",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-scss": "2.1.1",
    "prettier": "^2.3.1",
    "scully-plugin-minify-html": "^3.1.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.4",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "tabWidth": 2,
    "useTabs": true,
    "singleQuote": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid"
  }
}
